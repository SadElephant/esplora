{
    "name": "esplora/esplora",
    "description": "Easy and open-source website analytics tool for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandr Chernyaev",
            "email": "bliz48rus@gmail.com",
            "homepage": "https://tabuna.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "ext-redis": "*",
        "ext-pcntl": "*",
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.32",
        "jenssegers/agent": "^2.6"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench-core": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Esplora\\Analytics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Esplora\\Analytics\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Esplora\\Analytics\\EsploraServiceProvider"
            ],
            "aliases": {
                "Tracker": "Esplora\\Analytics\\Facades\\Tracker"
            }
        }
    }
}
